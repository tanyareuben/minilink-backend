{
  "name": "yum",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "db:migrate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:seed": "npx tsx api/db/seed.ts",
    "db:drop": "npx drizzle-kit sql 'DROP TABLE IF EXISTS drizzle_migrations CASCADE; DROP SCHEMA public CASCADE; CREATE SCHEMA public;'",
    "dev": "npx tsx watch --clear-screen=false src/index.ts",
    "start": "node dist/api/index.js",
    "ts.check": "tsc -p tsconfig.json"
  },
  "pre-commit": [
    "ts.check",
    "build"
  ],
  "keywords": [],
