    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^4.0.0",
    "bcrypt": "^5.1.1",
    "cli-color": "^2.0.4",
    "drizzle-orm": "^0.33.0",
    "fastify": "^4.28.0",
    "fastify-type-provider-zod": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.8",
    "nodemailer": "^6.9.15",
