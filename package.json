    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.5.5",
    "@types/nodemailer": "^6.4.16",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.24.2",
